ndles/DataTables", "~/Scripts/datepicker/moment.min.js", "~/Scripts/datepicker/bootstrap-datepicker.js", "~/Scripts/bootstrap-switch.js", "~/Scripts/alertify/alertify.js", "~/Content/timepicker/timepicker.js")        @RenderSection("Scripts", required: false)    <script type="text/javascript">        var domain = '@(SiteKeys.MainDomain)';    </script>    <!-- Bootstrap 3.3.7 --></body></html>