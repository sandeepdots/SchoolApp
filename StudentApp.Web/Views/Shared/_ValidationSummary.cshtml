@model IEnumerable<ModelError>

@if (Model.Count() > 0)
{
    <div class="validation-summary-errors" data-valmsg-summary="true">
        <ul>
            @foreach (var item in Model)
            {
                <li style="color:#A94442;">@item.ErrorMessage</li>
            }
        </ul>
    </div>
}
