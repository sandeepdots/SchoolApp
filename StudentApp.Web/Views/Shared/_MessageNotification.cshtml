@{
        var model = (ViewData["NotificationModel"] ?? TempData["NotificationModel"]) as SchoolApp.Web.Models.Notification;
    }

    @if (model != null)
    {
        <div id="messageBox" class="alert alert-@(model.Type.ToString().ToLower()) alert-dismissable">
            <button aria-hidden="true" data-dismiss="alert" class="close" type="button">×</button>
            @(model.Message)
        </div>
    }



